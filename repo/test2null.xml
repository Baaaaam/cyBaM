<?xml version="1.0"?>
<!-- 2 SourceFacility FrontEnds, 1 GenericRepository BackEnd, and 1 NullMarket -->
<simulation>
  <duration>10</duration>
  <startmonth>1</startmonth>
  <startyear>2000</startyear>
  <simstart>0</simstart>
  <decay>0</decay>

  <commodity>
    <name>uo2</name>
  </commodity>

  <market>
    <name>uo2market</name>
    <mktcommodity>uo2</mktcommodity>
    <model>
      <NullMarket/>
    </model>
  </market>

  <facility>
    <name>FrontEnd1</name>
    <model>
      <SourceFacility>
        <outcommodity>uo2</outcommodity>
        <capacity>.5</capacity>
        <inventorysize>1e4</inventorysize>
        <commodprice>0</commodprice>
        <recipe>fresh_50gwd</recipe>
      </SourceFacility>
    </model>
  </facility>

  <facility>
    <name>FrontEnd2</name>
    <model>
      <SourceFacility>
        <outcommodity>uo2</outcommodity>
        <capacity>.5</capacity>
        <inventorysize>1e4</inventorysize>
        <commodprice>0</commodprice>
        <recipe>fresh_50gwd</recipe>
      </SourceFacility>
    </model>
  </facility>
  <facility>
    <name>BackEnd</name>
    <model>
      <GenericRepository>
        <x>1</x>
        <y>1</y>
        <z>1</z>
        <dx>0.2</dx>
        <dy>0.2</dy>
        <dz>0.2</dz>
        <capacity>3</capacity>
        <incommodity>uo2</incommodity>
        <inventorysize>1e4</inventorysize>
        <lifetime>1</lifetime>
        <startOperMonth>1</startOperMonth>
        <startOperYear>1</startOperYear>
        <component>
          <name>ff</name>
          <componenttype>FF</componenttype>
          <thermalmodel>
            <StubThermal/>
          </thermalmodel>
          <nuclidemodel>
            <StubNuclide/>
          </nuclidemodel>
        </component>
        <component>
          <name>buffer</name>
          <componenttype>BUFFER</componenttype>
          <thermalmodel>
            <StubThermal/>
          </thermalmodel>
          <nuclidemodel>
            <StubNuclide/>
          </nuclidemodel>
        </component>
        <component>
          <name>wp</name>
          <componenttype>WP</componenttype>
          <thermalmodel>
            <StubThermal/>
          </thermalmodel>
          <nuclidemodel>
            <StubNuclide/>
          </nuclidemodel>
          <allowedwf>wf</allowedwf>
        </component>
        <component>
          <name>wf</name>
          <componenttype>WF</componenttype>
          <thermalmodel>
            <StubThermal/>
          </thermalmodel>
          <nuclidemodel>
            <StubNuclide/>
          </nuclidemodel>
          <allowedcommod>uo2</allowedcommod>
        </component>
      </GenericRepository>
    </model>
  </facility>

  <region>
    <name>oneRegion</name>
    <allowedfacility>FrontEnd1</allowedfacility>
    <allowedfacility>FrontEnd2</allowedfacility>
    <allowedfacility>BackEnd</allowedfacility>
    <model>
      <NullRegion/>
    </model>
    <institution>
      <name>oneInst</name>
      <model>
        <FixedInst>
          <facility>
            <type>FrontEnd1</type>
            <name>FrontEnd1</name>
          </facility>
          <facility>
            <type>FrontEnd2</type>
            <name>FrontEnd2</name>
          </facility>
          <facility>
            <type>BackEnd</type>
            <name>BackEnd</name>
          </facility>
        </FixedInst>
      </model>
    </institution>
  </region>

  <recipe>
    <name>fresh_50gwd</name>
    <basis>mass</basis>
    <unit>assembly</unit>
    <total>1000</total>
    <isotope>
      <id>92235</id>
      <comp>0.05</comp>
    </isotope>
    <isotope>
      <id>92238</id>
      <comp>0.95</comp>
    </isotope>
    <isotope>
      <id>08016</id>
      <comp>0.13</comp>
    </isotope>
  </recipe>

</simulation>
